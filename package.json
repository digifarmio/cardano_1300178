{
  "name": "digifarm-api",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "deploy": "serverless deploy",
    "lint": "eslint src --ext .ts --config eslint.config.cjs",
    "lint:fix": "eslint src --ext .ts --fix --config eslint.config.cjs",
    "format": "prettier --write src/**/*.ts",
    "dev": "serverless offline start",
    "debug": "serverless offline start --debug",
    "start": "NODE_OPTIONS='--enable-source-maps' serverless offline start",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:api": "jest --testPathPattern=api",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "generate-admin-token": "npx ts-node -r tsconfig-paths/register scripts/generateAdminToken.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.812.0",
    "@aws-sdk/client-s3": "^3.812.0",
    "@aws-sdk/client-sqs": "^3.812.0",
    "@aws-sdk/util-dynamodb": "^3.812.0",
    "@serverless/utils": "^6.15.0",
    "axios": "^1.9.0",
    "csv-writer": "^1.6.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "p-limit": "^6.2.0",
    "p-retry": "^6.2.1",
    "serverless-http": "^3.2.0",
    "ssh2-sftp-client": "^12.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@types/aws-lambda": "^8.10.149",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.16",
    "@types/ssh2-sftp-client": "^9.0.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "aws-sdk-client-mock": "^4.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^3.5.3",
    "serverless-offline": "^14.4.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
